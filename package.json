{
  "name": "intelligent-shopping-assistant",
  "version": "1.0.0",
  "description": "AI-powered shopping assistant with rule-based reasoning system",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "dev": "PORT=3011 nodemon backend/server.js",
    "client": "cd frontend && npm run serve",
    "dev:full": "concurrently \"npm run dev\" \"npm run client\"",
    "start-all": "concurrently \"NODE_ENV=production MONGODB_URI=mongodb://localhost:27017/shopping_assistant npm start\" \"cd frontend && npm run serve\"",
    "test": "jest --coverage",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up",
    "docker:down": "docker-compose down",
    "create-admin": "node backend/scripts/createAdmin.js",
    "create-orders": "node backend/scripts/create-orders.js",
    "seed": "node backend/scripts/seedDatabase.js"
  },
  "keywords": [
    "shopping-assistant",
    "rule-based-system",
    "design-patterns",
    "blackboard-architecture"
  ],
  "author": "SE Team",
  "license": "MIT",
  "dependencies": {
    "@vue/cli-service": "^5.0.9",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "helmet": "^7.2.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^8.0.3",
    "socket.io": "^4.6.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "nodemon": "^3.1.11",
    "supertest": "^6.3.3"
  }
}
